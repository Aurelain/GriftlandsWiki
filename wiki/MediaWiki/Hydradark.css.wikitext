/* wiki editor styling */
div.wikiEditor-ui {
    background: rgba(0, 0, 0, 0);
    border: none;
}

div.wikiEditor-ui-tabs div {
    border-bottom: 1px solid #080808;
    border-right: 1px solid #080808;
