p {
  font-family: "OpenSans-Regular";
}
 
/* Hide main page header */
body.page-Griftlands_Wiki h1.firstHeading {
    display:none;
}

/* Page Headings */
.mw-body .firstHeading,
.mw-body h1, 
.mw-body h2,
h1, h2, h3, h4, h5, h6 {
	border-bottom-color: #FFFFFF;
	color: #FFFFFF;
}

/* Wiki logo */
#p-logo a {
  background-position-x: 50%;
  background-position-y: 35%;
  background-size: 100% auto;
  background-repeat: no-repeat;
  display: block;
  height: 100%;
  text-decoration-color: currentcolor;
  text-decoration-line: none;
  text-decoration-style: solid;
  width: 20em;
  -webkit-filter: drop-shadow(0px 0px 10px #000000);
  filter: drop-shadow(0px 0px 3.5px #000000) drop-shadow(1px 1px 1px #000000);
}

/***********/
/* Anchors */
/***********/
a, 
a:visited,
div#content a.extiw,
div#content a.extiw:visited,
div#content a.external, 
div#content a.external:visited {
    color: #57FFFB;
    font-family: "OpenSans-Regular";
}

label.toctogglelabel { 
    color: #57FFFB;
}

a:hover {
  text-shadow: 0 0 4px;
  text-decoration:none;
}

/***********************/
/* Tabs top navigation */
/***********************/
#right-navigation {
  float: right;
  margin-bottom: -2.5em;
  margin-right: 1em;
  margin-top: 8.8em;
}

#left-navigation {
  display: inline;
  margin-left: 11em;
  float: left;
  margin-top: 8.8em;
  margin-bottom: -2.5em;
}


/* navigation links */
div.vectorTabs li a, 
div.vectorTabs li a span, 
div.vectorMenu li a,
div#mw-head li.new a, 
div#mw-head li.new a span,
div.vectorMenu h3 span {
  color: #08e0db;
  font-size: 14px;
  text-shadow:  1px  1px 1px black,
                1px -1px 1px black,
               -1px  1px 1px black,
               -1px -1px 1px black;
 font-family: "OpenSans-ExtraBold";
}

div.vectorMenu h3 span {
  display: block;
  font-weight: normal;
  margin-right: 20px;
  padding-left: 0.7em;
  padding-top: 1.2em;
}

div.vectorTabs li.selected a, 
div.vectorTabs li.selected a:visited {
  color: gray;
}
 
div#mw-panel div.portal div.body ul li a:link,
div#mw-panel div.portal div.body ul li a:visited {
  color: white;
  font-family: "OpenSans-Regular";
}

/* bg of navbar tabs */
div.vectorTabs li.selected,
div.vectorTabs ul li {
  background-color: transparent;
  background-image: none;
}

/* removes the divider */
div.vectorTabs, 
div.vectorTabs ul, 
div.vectorTabs span, 
div#mw-head div.vectorMenu h3 {
  background-image: none;
}

div.vectorMenu div.menu {
  border: none;
}

/*************/
/* wiki body */
/*************/
body {
    background: url("https://static.wikia.nocookie.net/griftlands_gamepedia_en/images/d/dc/Griftlands_wiki_background.jpg/revision/latest");
    background-position: center top;
    background-attachment: fixed;
    background-repeat: no-repeat;
    background-color: #623367;
    background-size: cover;
}

#mw-page-base {
     background-color: transparent;
     background-image: none;
     background-position: left bottom;
     background-repeat: repeat-x;
}

.mw-body {
  border:none;
  direction: ltr;
  margin-top: 100px;
}

div#pageWrapper {
        background: rgba(5, 58, 64, 0.8);
	border: none;
	margin-top: 7em;
	min-height: 50em;
        border-top: 1px solid #57FFFB;
}

div#content {
  background: rgba(0, 0, 0, 0);
  border: none;
  margin-top: 2em;
}

div#footer {
  background: rgba(0, 0, 0, 0);
  margin-left: 11em;
  margin-top: 10px;
  padding-bottom: 1.25em;
  padding-left: 1.25em;
  padding-right: 1.25em;
  padding-top: 1.25em;
  border:none;
}

div#footer ul li {
  color: white;
}

/***********/
/* sidebar */
/***********/
div#mw-panel {
  font-size: inherit;
  left: 0;
  padding-top: 2em;
  position: absolute;
  top: 160px;
  width: 10em;
}

.skin-hydradark #mw-panel.collapsible-nav div.portal h3 a,
.skin-hydradark #mw-panel.collapsible-nav .portal h3 a,
.skin-hydradark div#mw-panel div.portal h3 {
  color: #08e0db;
  font-size: 14px;
  text-shadow:  1px  1px 1px black,
                1px -1px 1px black,
               -1px  1px 1px black,
               -1px -1px 1px black;
  font-family: "OpenSans-ExtraBold";
}

#p-navigation div.body, 
#p-tb div.body, 
#p-Gamepedia div.body,
#p-More_by_KLEI div.body,
#p-Top_Pages div.body {
  background: rgba(0, 0, 0,0.2);
  border:none;
  margin-left: 5px !important;
  margin-right: 5px !important;
  padding-bottom: 4px;
  padding-left: 4px;
  padding-right: 4px;
}

/* for "edit this page" tab and "discussion" tab etc */
.pagetab {
     border-style: solid solid none;
     border-width: thin;
     border-color: #808080;
     padding: 0.25ex 1ex 0ex;
     font-size: 95%;
}

.ambox {
    background-color: #282828;
    border-bottom-color: #383838;
    border-right-color: #383838;
    border-top-color: #383838;
    border-collapse: collapse;
    font-size: 95%;
    margin: 0 auto 2px auto;
    width: 80%;
}

.ambox-gray {
    border-left-color: #383838;
}

.ambox.ambox-tiny {
    font-size: 90%;
    margin: 2px 0;
    width: auto;
}

.ambox + .ambox {
     margin-top: -2px;
}

.ambox-text {
     padding: 0.25em 0.5em;
}

.ambox-image {
     padding: 2px 0px 2px 0.5em;
     text-align: center;
     width: 60px;
}

.ambox-tiny .ambox-image {
     padding: 2px 0.5em;
     text-align: left;
     width: auto;
}

/* Ambox colors */
.ambox-blue {
     border-left: 10px solid #1e90ff;
}

.ambox-red {
     border-left: 10px solid #b22222;
}

.ambox-orange {
     border-left: 10px solid #f28500;
}

.ambox-yellow {
     border-left: 10px solid #f4c430;
}

.ambox-purple {
     border-left: 10px solid #9932cc;
}

.ambox-gray {
     border-left: 10px solid #bba;
}

.ambox-green {
     border-left: 10px solid #228b22;
}

/* Ambox small text */
.amsmalltext {
     font-size: smaller;
     margin-left: 0.8em;
     margin-top: 0.5em;
}

/* INFOBOXES: game or book depended color style */
.infoboxtable {
    background-image: url(https://static.wikia.nocookie.net/griftlands_gamepedia_en/images/9/91/IBbg.png/revision/latest?cb=20200919191628);
    border: 2px solid;
    border-top-color: #08e0db;
    border-bottom-color: #08e0db;
    border-left-color: transparent;
    border-right-color: transparent;
    border-radius:5px;
    color: #08e0db;
    float: right;
    font-size: 89%; 
    margin-bottom: 0.5em; 
    margin-left: 1em; 
    padding: 0.2em; 
    width: 300px;
}

.infoboxtable td {
    vertical-align: top;
}

.infoboxtable td > div {
    background-color: rgba(87, 255, 251,0.3); 
    border: 2px solid inherit; 
    border-radius: 5px; 
    font-weight: bold;
    padding: 0.5px 7px;
    text-align: right;
    color: #ffffff;
}

.infoboxname {
    background: radial-gradient(circle at 145px 10px, rgba(87, 255, 251,0.4), transparent); 
    color: #ffffff;
    font-size: 110%; 
    font-weight: bold;
    padding: 0.5em; 
}

.infoboxdetails {
    padding: 0em;
    background: radial-gradient(circle at 145px 10px, rgba(87, 255, 251,0.4), transparent); 
}

.infoboxover {
	background-color: rgba(87,255,251,0.4);
	border-top-color: transparent;
	border-bottom-color: transparent;
	border-left-color: #57FFFB;
	border-right-color: transparent;
	padding: 5px;
	color: #57FFFB;
	text-transform: uppercase;
	border: 2px solid #57FFFB;
	border-bottom: 0;
	border-top: 0;
	border-right: 0;
}


/* copyright images tweak */
.skin-hydradark .copyright-icon {
    background-image: url("https://static.wikia.nocookie.net/commons_hydra/images/thumb/5/58/Copyright_darkwiki.png/32px-Copyright_darkwiki.png");
    background-size: 100%;
    height: 32px;
    width: 32px;
}

.feature {
    background: rgba(0, 0, 0, 0.1);
    border: 1px solid rgba(0, 0, 0, 0.5);
    border-radius: 5px; padding: 10px;
}

/*************************/
/* Navbox template style */
/*************************/
table.navbox {
    border: 1px solid #67676;
    clear: both;
    font-size: 88%;
    margin: auto;
    padding: 1px;
    text-align: center;
    width: 100%; 
}

/* Border between adjacent navboxes */
table.navbox + table.navbox {
    margin-top: -1px;
}

.navbox-title,
.navbox-abovebelow,
table.navbox th {
    padding-left: 1em;
    padding-right: 1em;
    text-align: center;
}

.navbox-group {
    font-weight: bold;
    padding-left: 1em;
    padding-right: 1em;
    white-space: nowrap;
}

/* Base background */
.navbox, .navbox-subgroup {
    background: rgba(0, 0, 0, 0.1);
}

.navbox-list {
    border-color: rgba(0, 0, 0, 0.1); /* Must match background color */
}

/* Level 1 color */
.navbox-title,
table.navbox th {
    background: rgba(0, 0, 0, 0.2);
    color: #fff;
}

/* Level 2 styling */
.navbox-abovebelow,
.navbox-group,
.navbox-subgroup .navbox-title {
    background: rgba(0, 0, 0, 0.2);
    color: #fff;
}

/* Level 3 styling */
.navbox-subgroup .navbox-group,
.navbox-subgroup .navbox-abovebelow {
    background: rgba(0, 0, 0, 0.1);
    color: #fff;
}

/* Even row striping */
.navbox-even {
    background: rgba(255, 255, 255, 0.1);
    color: #fff;
}

/* Odd row striping */
.navbox-odd {
    background: transparent;
}
 
.collapseButton {
    font-weight: normal;
    width: auto;
}

.navbox .collapseButton {
    width: 6em;
}
 
.navbar {
    font-size: 88%;
    font-weight: normal;
}

.navbox .navbar {
    font-size: 100%;
}
 
table.collapsed tr.collapsible {
    display: none;
}
/*****************************/
/* End of new Navbox styling */
/*****************************/


/********************************/
/* Front page appearance styles */
/********************************/
.fpbox .welcome {
    text-shadow:1px  1px 1px black,
                1px -1px 1px black,
               -1px  1px 1px black,
               -1px -1px 1px black;
    border-bottom: 1px solid #57fffb;
    color: #D0FCFE; /* change for hydra (light) wikis */
    font-size: 150%;
    margin: 0 0 10px 0;
    padding: 0 0 5px 0;
    text-transform: uppercase;
    text-align: center;
    font-family: "OpenSans-Bold";
    background-color: transparent;
}

.fpbox .heading {
    text-shadow:1px  1px 1px black,
                1px -1px 1px black,
               -1px  1px 1px black,
               -1px -1px 1px black;
    border-bottom: 1px solid #57fffb;
    color: #D0FCFE; /* change for hydra (light) wikis */
    font-size: 132%;
    font-variant: small-caps;
    margin: 0 0 10px 0;
    padding: 0 0 5px 0;
    font-family: "OpenSans-Bold";
    background-color: transparent;
}

.fpbox {
  background: rgba(0, 0, 0,0.2);
  border-bottom-color: #57fffb;
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 15px;
  border-bottom-style: solid;
  border-bottom-width: 1px;
  border-image-outset: 0 0 0 0;
  border-image-repeat: stretch stretch;
  border-image-slice: 100% 100% 100% 100%;
  border-image-source: none;
  border-image-width: 1 1 1 1;
  border-left-color: transparent;
  border-left-style: solid;
  border-left-width: 1px;
  border-right-color: transparent;
  border-right-style: solid;
  border-right-width: 1px;
  border-top-color: #57fffb;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  border-top-style: solid;
  border-top-width: 1px;
  box-shadow: 0 0.1em 0.75em #57fffb;
  color: #ccfdfe;
  margin-bottom: 5px;
  margin-left: 5px;
  margin-right: 5px;
  margin-top: 5px;
  padding-bottom: 5px;
  padding-left: 5px;
  padding-right: 5px;
  padding-top: 5px;
}

.fplink:not(.fplink-plain) {
    background: #202020; 
    background-image: -moz-linear-gradient(center top, #202020 0%, #505050 100%); 
    background-image: -webkit-gradient(linear, center top, left bottom, color-stop(0%,#202020), color-stop(100%,#505050)); 
    background-image: -webkit-linear-gradient(top, #202020 0%, #505050 100%); 
    background-image: -o-linear-gradient(top, #202020 0%, #505050 100%); 
    background-image: -ms-linear-gradient(top, #202020 0%, #505050 100%); 
    background-image: linear-gradient(to bottom, #202020 0%, #505050 100%); 
    border-top: 1px solid #57fffb;;
    border-bottom: 1px solid #57fffb;
    border-radius: 4px;
    color: #000000;
}

.fplink:not(.fplink-plain):hover {
 -webkit-filter: drop-shadow(0px 0px 10px #57fffb);
    filter: drop-shadow(0px 0px 3.5px #57fffb) drop-shadow(1px 1px 1px #57fffb);
}

.fplink-fullwidth .fplink:not(.fplink-plain) {
    background: #202020; 
    background-image: -moz-linear-gradient(center top, #202020 0%, #505050 100%); 
    background-image: -webkit-gradient(linear, center top, left bottom, color-stop(0%,#202020), color-stop(100%,#505050)); 
    background-image: -webkit-linear-gradient(top, #202020 0%, #505050 100%); 
    background-image: -o-linear-gradient(top, #202020 0%, #505050 100%); 
    background-image: -ms-linear-gradient(top, #202020 0%, #505050 100%); 
    background-image: linear-gradient(to bottom, #202020 0%, #505050 100%); 
    border-top: 1px solid #57fffb;;
    border-bottom: 1px solid #57fffb;
    border-radius: 4px;
    color: #000000;
}

.linkslabel {
    border-bottom: 2px solid #505050;
}
